{
    "buildCommand": "npm run build",
    "outputDirectory": "public",
    "functions": {
        "server/index.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server/index.js"
        }
    ]
}